html
	head
		link(rel='stylesheet' href='/reset.css')
		style
			:sass
				canvas, .front
					position: absolute
					left: 0
					top: 0
					width: 100%
					height: 100%

				.front
					padding: 30px
					font-family: monospace
					color: #242424

	body

		script.

			class Test {

				constructor() {

					this.a = Math.random()
					this.b = Math.random()
					this.c = Math.random()
					this.d = Math.random()
					this.e = Math.random()
					this.f = Math.random()
					this.g = Math.random()
					this.h = Math.random()

				}

				test1() {

					return this.a * this.b * this.c * this.d + this.a * this.b * this.c * this.d + 
						this.e * this.f * this.g * this.h + this.e * this.f * this.g * this.h
					
				}

				test2() {

					let { a, b, c, d, e, f, g, h } = this

					return a * b * c * d + a * b * c * d + 
						e * f * g * h + e * f * g * h

				}

			}

			let test = new Test()
			let n = 100000e3
			let t

			console.log(n)

			t = -performance.now()

			for (let i = 0; i < n; i++)
				test.test1()

			t += performance.now()
			console.log('test1', t, t / n)



			t = -performance.now()

			for (let i = 0; i < n; i++)
				test.test2()

			t += performance.now()
			console.log('test2', t, t / n)

